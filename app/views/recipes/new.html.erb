<%= form_for @recipe do |f| %>
    <%= f.label "Title:" %>
    <%= f.text_field :title %><br>

    <%= f.fields_for :ingredients do |f2| %>
        <%= f2.label :name %>
        <%= f2.text_field :name %><br>

        <%= f2.label :quantity %>
        <%= f2.text_field :quantity %>
    <% end %>

    <%= f.submit %>
<% end %>